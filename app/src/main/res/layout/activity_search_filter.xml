<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pavan.cardoctor.SearchFilter">

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maruti"
        android:id="@+id/checkBox"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="89dp"
        android:checked="false"
        android:hint="Maruti" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Honda"
        android:id="@+id/checkBox2"
        android:layout_below="@+id/checkBox"
        android:layout_centerHorizontal="true"
        android:hint="Honda"
        android:checked="false"
        android:layout_alignLeft="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tata Motors"
        android:id="@+id/checkBox3"
        android:layout_below="@+id/checkBox2"
        android:layout_centerHorizontal="true"
        android:hint="Tata Motors"
        android:checked="false"
        android:layout_alignLeft="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hyundai"
        android:id="@+id/checkBox4"
        android:layout_below="@+id/checkBox3"
        android:layout_centerHorizontal="true"
        android:checked="false"
        android:hint="Hyundai"
        android:layout_alignLeft="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Toyota"
        android:id="@+id/checkBox5"
        android:layout_below="@+id/checkBox4"
        android:layout_centerHorizontal="true"
        android:checked="false"
        android:hint="Toyota"
        android:layout_alignLeft="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chevrolet"
        android:id="@+id/checkBox6"
        android:layout_below="@+id/checkBox5"
        android:layout_centerHorizontal="true"
        android:hint="Chevrolet"
        android:checked="false"
        android:layout_alignLeft="@+id/checkBox"
        android:layout_alignStart="@+id/checkBox"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apply"
        android:id="@+id/button16"
        android:layout_below="@+id/checkBox6"
        android:layout_alignLeft="@+id/checkBox6"
        android:layout_alignStart="@+id/checkBox6"
        android:layout_marginTop="89dp" />
</RelativeLayout>
